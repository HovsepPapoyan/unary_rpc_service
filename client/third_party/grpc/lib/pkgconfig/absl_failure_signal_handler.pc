prefix=/grpc_install
exec_prefix=${prefix}
libdir=/grpc_install/lib
includedir=/grpc_install/include

Name: absl_failure_signal_handler
Description: Abseil failure_signal_handler library
URL: https://abseil.io/
Version: 20250512
Requires: absl_examine_stack = 20250512, absl_stacktrace = 20250512, absl_base = 20250512, absl_config = 20250512, absl_core_headers = 20250512, absl_raw_logging_internal = 20250512
Libs: -L${libdir}  -labsl_failure_signal_handler 
Cflags: -I${includedir} -Wno-float-conversion -Wno-implicit-float-conversion -Wno-implicit-int-float-conversion -Wno-unknown-warning-option -DNOMINMAX
