prefix=/grpc_install
exec_prefix=${prefix}
libdir=/grpc_install/lib
includedir=/grpc_install/include

Name: absl_utf8_for_code_point
Description: Abseil utf8_for_code_point library
URL: https://abseil.io/
Version: 20250512
Requires: absl_config = 20250512
Libs: -L${libdir}  -labsl_utf8_for_code_point 
Cflags: -I${includedir} -Wno-float-conversion -Wno-implicit-float-conversion -Wno-implicit-int-float-conversion -Wno-unknown-warning-option -DNOMINMAX
