prefix=/grpc_install
exec_prefix=${prefix}
libdir=/grpc_install/lib
includedir=/grpc_install/include

Name: absl_log_initialize
Description: Abseil log_initialize library
URL: https://abseil.io/
Version: 20250512
Requires: absl_config = 20250512, absl_log_globals = 20250512, absl_log_internal_globals = 20250512, absl_time = 20250512
Libs: -L${libdir}  -labsl_log_initialize 
Cflags: -I${includedir} -Wno-float-conversion -Wno-implicit-float-conversion -Wno-implicit-int-float-conversion -Wno-unknown-warning-option -DNOMINMAX
