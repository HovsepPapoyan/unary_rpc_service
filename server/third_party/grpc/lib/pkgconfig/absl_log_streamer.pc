prefix=/grpc_install
exec_prefix=${prefix}
libdir=/grpc_install/lib
includedir=/grpc_install/include

Name: absl_log_streamer
Description: Abseil log_streamer library
URL: https://abseil.io/
Version: 20250512
Requires: absl_config = 20250512, absl_absl_log = 20250512, absl_log_severity = 20250512, absl_optional = 20250512, absl_strings = 20250512, absl_strings_internal = 20250512, absl_utility = 20250512
Libs: -L${libdir}  
Cflags: -I${includedir} -Wno-float-conversion -Wno-implicit-float-conversion -Wno-implicit-int-float-conversion -Wno-unknown-warning-option -DNOMINMAX
