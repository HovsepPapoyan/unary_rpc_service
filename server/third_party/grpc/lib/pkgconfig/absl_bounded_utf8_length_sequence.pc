prefix=/grpc_install
exec_prefix=${prefix}
libdir=/grpc_install/lib
includedir=/grpc_install/include

Name: absl_bounded_utf8_length_sequence
Description: Abseil bounded_utf8_length_sequence library
URL: https://abseil.io/
Version: 20250512
Requires: absl_bits = 20250512, absl_config = 20250512
Libs: -L${libdir}  
Cflags: -I${includedir} -Wno-float-conversion -Wno-implicit-float-conversion -Wno-implicit-int-float-conversion -Wno-unknown-warning-option -DNOMINMAX
