prefix=/grpc_install
exec_prefix=${prefix}
libdir=/grpc_install/lib
includedir=/grpc_install/include

Name: absl_non_temporal_memcpy
Description: Abseil non_temporal_memcpy library
URL: https://abseil.io/
Version: 20250512
Requires: absl_non_temporal_arm_intrinsics = 20250512, absl_config = 20250512, absl_core_headers = 20250512
Libs: -L${libdir}  
Cflags: -I${includedir} -Wno-float-conversion -Wno-implicit-float-conversion -Wno-implicit-int-float-conversion -Wno-unknown-warning-option -DNOMINMAX
