prefix=/grpc_install
exec_prefix=${prefix}
libdir=/grpc_install/lib
includedir=/grpc_install/include

Name: absl_span
Description: Abseil span library
URL: https://abseil.io/
Version: 20250512
Requires: absl_algorithm = 20250512, absl_config = 20250512, absl_core_headers = 20250512, absl_nullability = 20250512, absl_throw_delegate = 20250512, absl_type_traits = 20250512, absl_weakly_mixed_integer = 20250512
Libs: -L${libdir}  
Cflags: -I${includedir} -Wno-float-conversion -Wno-implicit-float-conversion -Wno-implicit-int-float-conversion -Wno-unknown-warning-option -DNOMINMAX
